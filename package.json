{
  "name": "mongo_bulk",
  "version": "0.0.1",
  "description": "Wrapper around mongodb's bulkWrite for more intuitive bulk operations",
  "main": "index.js",
  "author": "Jeremy Bini",
  "license": "ISC",
  "keywords": [
    "mongo",
    "mongodb",
    "bulk",
    "bulkWrite"
  ],
  "engines": {
    "node": ">= 6.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "babel src -d dist",
    "lint": "eslint src tests",
    "lint:fix": "npm run lint -- --fix",
    "test": "MONGO_URL=mongodb://localhost:27017/MongoBulk mocha test --compilers js:babel-register --timeout=15000",
    "test:dev": "npm run test -- --watch",
    "prepublish": "npm run clean & npm run build"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "bluebird": "^3.5.0",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.25",
    "monk": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.18.0",
    "chance": "^1.0.6",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "expect": "^1.20.2"
  }
}
